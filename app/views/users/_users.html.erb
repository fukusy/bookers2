<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
      <% users.each do |user| %>
    <tr>
      <td>
        <%= link_to user_path(user) do %>
          <%= user.name %><br>
        <% end %>
      </td>
    </tr>
      <% end %>
  </tbody>
</table>