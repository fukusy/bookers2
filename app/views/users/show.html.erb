<div class='container px-5 px-sm-0'>
  <div class="row">


    <!--部分テンプレート-->
    <%= render 'users/list', user:@user %>
    <!--部分テンプレートおわり-->






<!--ユーザーが投稿したbooks-->
<% @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
  <p>title：<%= book.body %></p>
  <% end %>
  <p>投稿ユーザー画像：<%= image_tag('sample-author1.jpg') %></p>
  <p>ショップ名：<%= book.title  %></p>
  <p>ユーザーネーム：<%= book.user.name %></p>
</div>
<% end %>
<%= paginate @books %>

</div>
</div>